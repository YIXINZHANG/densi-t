<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginTop="15dp"
    android:layout_marginBottom="15dp">

    <TextView
        android:id="@+id/buildingName"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginLeft="10dp"
        android:textSize="15dp"
        android:layout_gravity="center_vertical|center_horizontal|left"/>
    <!--android:layout_weight="1"-->

    <com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar
        android:id="@+id/progress"
        android:layout_height="30dp"
        android:layout_width="230dp"
        android:layout_gravity="center_vertical|center_horizontal|center"/>
    <!--android:layout_weight="2"-->

    <TextView
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/busyness"
        android:layout_gravity="center_vertical|center_horizontal|right"/>
    <!--android:layout_weight="1"-->

</LinearLayout><!-- From: file:/C:/Users/pbz18_000/Desktop/Densi-T/densi-t/src/main/res/layout/listview_item.xml -->