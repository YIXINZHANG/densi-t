<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginTop="15dp"
    android:layout_marginBottom="15dp"
    android:weightSum="1">

    <TextView
        android:id="@+id/buildingName"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginLeft="10dp"
        android:textSize="11dp"
        android:textColor="#747474"
        android:layout_gravity="center_vertical|center_horizontal|left"/>
    <!--android:layout_weight="1"-->

    <com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar
        android:id="@+id/progress"
        android:layout_height="30dp"
        android:layout_width="192dp"
        android:layout_gravity="center_vertical|center_horizontal|center"
        android:transitionGroup="false" />
    <!--android:layout_weight="2"-->

    <!--<TextView-->
        <!--android:layout_width="35dp"-->
        <!--android:layout_height="45dp"-->
        <!--android:textSize="20dp"-->
        <!--android:layout_marginLeft="10dp"-->
        <!--android:id="@+id/busyness"-->
        <!--android:layout_gravity="center_vertical|center_horizontal|right"/>-->

    <ImageView
        android:layout_width="24dp"
        android:layout_height="26dp"
        android:id="@+id/clickfavourite"
        android:layout_gravity="center_vertical|center_horizontal|right"
        android:layout_weight="0.16"
        android:layout_marginLeft="70dp"
        android:background="@drawable/unfavourite"
        android:clickable="true"/>
